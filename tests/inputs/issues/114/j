# echo "js 3" > foo/under_foo.js

cloc --yaml --out outputs/issues/114/T1.yaml --by-file inputs/issues/114
cloc --yaml --out outputs/issues/114/T2.yaml --by-file --not-match-d bar inputs/issues/114
cloc --yaml --out outputs/issues/114/T3.yaml --by-file --not-match-d bee inputs/issues/114
cloc --yaml --out outputs/issues/114/T4.yaml --by-file --not-match-d bar/bee inputs/issues/114
cloc --yaml --out outputs/issues/114/T5.yaml --by-file --fullpath --not-match-d   bar inputs/issues/114
cloc --yaml --out outputs/issues/114/T6.yaml --by-file --fullpath --not-match-d ./bar inputs/issues/114
cloc --yaml --out outputs/issues/114/T7.yaml --by-file --fullpath --not-match-d bar/bee inputs/issues/114
